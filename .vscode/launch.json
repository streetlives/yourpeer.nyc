{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug server-side (https/local)",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev -- --experimental-https",
      "env": {
        "NEXT_PUBLIC_GO_GETTA_PROD_URL": "http://localhost:3001"
      },
      "envFile": "${workspaceFolder}/.env.local"
    },
    {
      "name": "Next.js: debug server-side (https)",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev -- --experimental-https",
      "env": {
        "NEXT_PUBLIC_GO_GETTA_PROD_URL": "https://w6pkliozjh.execute-api.us-east-1.amazonaws.com/prod"
      },
      "envFile": "${workspaceFolder}/.env.local"
    },
    {
      "name": "Next.js: debug server-side (http)",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "env": {
        "NEXT_PUBLIC_GO_GETTA_PROD_URL": "https://w6pkliozjh.execute-api.us-east-1.amazonaws.com/prod"
      },
      "envFile": "${workspaceFolder}/.env.local"
    },
    {
      "name": "Next.js: debug client-side",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000"
    },
    {
      "name": "Next.js: debug full stack",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "serverReadyAction": {
        "pattern": "- Local:.+(https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    }
  ]
}
